
@using _3.Models
@{
    Layout = null;
    Data[] data = ViewBag.getall;
    Data find2 = ViewBag.find2;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h6>@ViewBag.kek</h6>
        <table>
            @foreach (Data d in data)
            {
            <tr>
                <td>@d.Id</td>
                <td>@d.Name</td>
                <td>@d.Spec</td>
                <td><a href="#">Edit</a> | <a href="/hw/delete/@d.Id">Delete</a></td>
            </tr>
            }
        </table>
        <a href="#">Add new</a>
    </div>
</body>
</html>
